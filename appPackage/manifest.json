{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.apta.botgrp",
    "developer": {
        "name": "Dev-Apta",
        "websiteUrl": "https://www.apta.id",
        "privacyUrl": "https://www.apta.id/privacy",
        "termsOfUseUrl": "https://www.apta.id/termofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "BOT GRP Langchain",
        "full": "BOT GRP Langchain"
    },
    "description": {
        "short": "Welcome to Bot GRP Langchain to start using this bot please type /start",
        "full": "Welcome to Bot GRP Langchain to start using this bot please type /start"
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupchat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false,
            "commandLists": [
                {
                    "scopes": [
                        "personal",
                        "team",
                        "groupchat"
                    ],
                    "commands": [
                        {
                            "title": "/ask",
                            "description": "Use AI to ask a question about GRP database"
                        },
                        {
                            "title": "/start",
                            "description": "To show this main menu card"
                        }
                    ]
                }
            ]
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}